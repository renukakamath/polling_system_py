pragma solidity >= 0.5.0 < 0.7.0;
contract polling{
 
struct votes{

	// State variables
	uint vote_id;
	
	uint voter_id;
	uint candidate_id;

	string vote_time;
	
	string hash_value;
	

}

 
votes []m;



function add_vote(uint vote_id,uint  voter_id,uint  candidate_id,string memory hash_value,string  memory vote_time) public{
	votes memory m1=votes(vote_id,voter_id,candidate_id,hash_value,vote_time);
	m.push(m1);
}



struct user{

	// State variables
	uint user_id;
	uint login_id;
	string first_name;
	string last_name;
	string place;
	string phone;
	string email;
	string number;
	string aadhar;

	string Voterid;
	

}

 
user []u;



function add_user(uint user_id,uint login_id,string memory first_name,string memory last_name,string memory place,string memory phone,string memory email,string memory number,string memory aadhar,string memory Voterid) public{
	user memory u1=user(user_id,login_id,first_name,last_name,place,phone,email,number,aadhar,Voterid);
	u.push(u1);
}




}




